{
  "name": "mdas-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run tw:dev & vite",
    "build": "npm run tw:build && tsc -b && vite build",
    "build:wasm": "node scripts/build-wasm.js",
    "build:wasm:win": "build-wasm.bat",
    "build:wasm:unix": "bash build-wasm.sh",
    "lint": "eslint .",
    "preview": "vite preview",
    "tw:dev": "tailwindcss -i ./src/index.css -o ./src/tailwind.css --watch",
    "tw:build": "tailwindcss -i ./src/index.css -o ./src/tailwind.css --minify"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/cli": "^4.1.15",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.15",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
